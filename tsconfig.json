{
  "compilerOptions": {
    "isolatedModules": true,
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "esnext",
    "types": [
      "jest"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
}