{
  "name": "Improve your experience on YouTube",
  "version": "0.0.0",
  "description": "Hide ads, and add faster playback rates",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/watch?*"
      ],
      "run_at": "document_start",
      "js": [
        "bundle.js"
      ]
    }
  ],
  "homepage_url": "https://github.com/seanpoulter/browser-extension-youtube",
  "manifest_version": 2
}
